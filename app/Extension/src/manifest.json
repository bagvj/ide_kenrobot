{
	"manifest_version": 2,
	"name": "啃萝卜",
	"description": "啃萝卜平台扩展插件",
	"author": "kenrobot.com",
	"app": {
		"background": {
			"page": "background.html"
		}
	},
	"icons": {
		"16": "image/logo.ico",
		"128": "image/logo.ico"
	},
	"permissions": [
		"serial",
		"fileSystem",
		"*://*.kenrobot.com/"
	],
	"externally_connectable": {
  		"matches": ["*://*.kenrobot.com/*"]
	},
	"update_url": "http://platform.kenrobot.com/extension-update.xml",
	"version": "0.1.17"
}